{
  "name": "google-books-search",
  "version": "1.0.0",
  "description": "![](https://img.shields.io/badge/license-MIT%20License-blue) ![](https://travis-ci.com/wilmararturo/google-books-search.svg?branch=main)",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "test": "npm run lint",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Wilmar Stephens",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "9.0.0",
    "compression": "^1.7.4",
    "express": "^4.16.3",
    "eslint": "5.12.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "morgan": "^1.10.0",
    "react-scripts": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilmararturo/google-books-search.git"
  },
  "bugs": {
    "url": "https://github.com/wilmararturo/google-books-search/issues"
  },
  "homepage": "https://github.com/wilmararturo/google-books-search#readme"
}
